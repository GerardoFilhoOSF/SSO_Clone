    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:dev": "set REACT_APP_SKIP_AUTH=true && set REACT_APP_ENABLE_ALL_MOCKS=true && react-app-rewired start",
    "build": "env-path -p .env.local react-app-rewired build",
    "build:dev": "env-path -p .env.development react-app-rewired build",
    "build:hom": "env-path -p .env.homologation react-app-rewired build",
    "build:release": "env-path -p .env.release react-app-rewired build",
    "build:prd": "env-path -p .env.production react-app-rewired build",
    "test": "set CI=true && react-app-rewired test",
    "test:watch": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
